html {
    height:100%;
}
body {
    padding-top: 50px;
    height:100%;
}

.hidden{display:none;}
.clear{clear:both;}

#left_col{background-color:#f5f5f5; padding-top: 1em; min-height: 250px;}
.right-col{height:100%; padding: 0; border-left: 1px solid #fff;}

.ui-timepicker-list {
    height: auto;
    max-height: 200px;
    width: 200px;
    overflow-x: hidden;
    background-color: #b9def0;
}

.features .editor .editorwrap {
    max-width: 2180px;
}
.features .editor .CodeMirror pre {
    padding-left:10px;
}
.features .editor .entry-preview-content {
    padding: 10px 20px 40px 10px;
}
.features .editor .entry-preview {
    border-left:5px;
}
.features .editor .CodeMirror-lines {
    padding: 10px 0 40px 0
}
#storage_key_load, #storage_key_save{
    width:200px;
}
#nav_save, #nav_load, #nav_login, #nav_register, #nav_request{
    background-color:#080808;
}

#nav_note{
    padding-top:3px;
    padding-left:3px;
}

#root_list, #root_list ul{
    list-style-type: none;
    padding-left: 1.4em;
}
#root_list li{
    min-height: 1.4em;
}

