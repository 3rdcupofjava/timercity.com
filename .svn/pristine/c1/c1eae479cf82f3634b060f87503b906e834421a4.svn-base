body {
    background: #efefef;
}

svg{
    stroke: #000;
    font-family: "HelveticaNeue-Light", "Helvetica Neue Light", "Helvetica Neue", Helvetica, Arial, "Lucida Grande", sans-serif;
}

#rim {
    fill: none;
    stroke: #999;
    stroke-width: 3px;
}

.second-hand{
    stroke-width:3;

}

.minute-hand{
    stroke-width:8;
    stroke-linecap:round;
}

.hour-hand{
    stroke-width:12;
    stroke-linecap:round;
}

.hands-cover{
    stroke-width:3;
    fill:#fff;
}

.second-tick{
    stroke-width:3;
    fill:#000;
}

.hour-tick{
    stroke-width:8; /*same as the miute hand*/
}

.second-label{
    font-size: 12px;
}

.hour-label{
    font-size: 24px;
}

#txts {
    background-color: #ffffff;
    border: solid;
}


.close{
    margin: -3px 3px 0px 0px;
}

#tabs{background-color: #e1e1e1;}
#ntm{padding: 10px;}
#tab-adder,#tab-remover{padding: 13px 5px 0px 5px;}

/* PHOTO STUFF */ 
.min_clock_holder{border-top: 1px solid #FFF;}
#buttons_lap_timer{float:left; display:none;}
.lapTimeHolder{margin-top: 10px;}
.lapTimeHolder > div{padding: 10px;}
.timer_holder{display: block;float:left;min-width:33.2%; padding:5px 0px; min-height: 550px;} 
.timer_holder{display: block;float:left;min-width:20%; padding:5px 0px; min-height: 550px;}
.timer_box{margin-top:20px;margin-left: 3px;display: block;position: relative;}/*min-width: 250px;}*/
.tb_nav{background-color: #fff; padding:2px;border:1px solid #e1e1e1;float:left;width:200px;}
.tb_nav a:hover{text-decoration: none;}
.tb_nav .title{text-overflow: ellipsis; overflow: hidden; white-space: nowrap; width: 70%;display:block;}
.tb_nav button{margin-top: -23px;}

li.timer_box{display:block;}
.timer_box a:hover {
    text-decoration: none;}
.timer_box .title{
    text-overflow: ellipsis;
    color: #428bca; 
    overflow: hidden;
    white-space: nowrap;
    width: 80%;     top:2px;     left:10px;     text-shadow: -1px 0 1px #FFF, 0 1px 1px #FFF, 1px 0 1px #FFF, 0 -1px 1px #FFF; }
.digital_display{
    margin-top:5px;
 	position: absolute;
    bottom: 5%;
    right: 5%;
    /*margin-top:5px;*/
    float: right;
    margin-right: 4px;
    text-align: right;
    background-color: #FFFFFF;
    z-index: 999999999;
    display: block;
    text-shadow: -1px 0 1px #FFF, 0 1px 1px #FFF, 1px 0 1px #FFF, 0 -1px 1px #FFF; 
}
.highlight{
    background-color: rgba(27,118,90,0.3);
}

